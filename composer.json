{
    "name": "fivesqrd/fluent-laravel",
    "description": "A Laravel 5 service provider for including the Fluent PHP library.",
    "keywords": ["laravel", "laravel 5"],
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Five Squared",
            "homepage":"http://fivesqrd.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "fivesqrd/fluent-frontier": "1.*",
        "laravel/framework": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "Fluent\\Laravel\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Fluent\\Laravel\\Test\\": "tests/" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fluent\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Fluent": "Fluent\\Laravel\\Facade"
            }
        }
    }
}
